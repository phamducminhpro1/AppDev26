<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AppDev" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#93B9F5</item>
        <item name="colorPrimaryVariant">#0E67B4</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#e12305</item>
        <item name="colorSecondaryVariant">#991703</item>
        <item name="colorOnSecondary">@color/black</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/roboto</item>
    </style>

    <color name="colorGreenStart">#72DFC5</color>
    <color name="colorGreenEnd">#1DDE7D</color>
    <color name="colorBlueStart">#6C92F4</color>
    <color name="colorBlueEnd">#1A73E9</color>
    <color name="colorFacebook">#3B5999</color>
    <color name="colorToolbar">@color/white</color>
    <!-- Chat colors -->
    <color name="colorSent">#93B9F5</color>
    <color name="colorReceived">#e6e6e6</color>
</resources>